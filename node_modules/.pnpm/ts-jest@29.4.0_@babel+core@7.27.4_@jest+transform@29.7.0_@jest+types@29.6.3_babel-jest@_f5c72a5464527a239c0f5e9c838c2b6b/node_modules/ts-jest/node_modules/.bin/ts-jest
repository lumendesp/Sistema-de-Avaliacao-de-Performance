#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/diogo/RocketLab/RPE/Sistema-de-Avaliacao-de-Performance/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_f5c72a5464527a239c0f5e9c838c2b6b/node_modules/ts-jest/node_modules:/home/diogo/RocketLab/RPE/Sistema-de-Avaliacao-de-Performance/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_f5c72a5464527a239c0f5e9c838c2b6b/node_modules:/home/diogo/RocketLab/RPE/Sistema-de-Avaliacao-de-Performance/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/diogo/RocketLab/RPE/Sistema-de-Avaliacao-de-Performance/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_f5c72a5464527a239c0f5e9c838c2b6b/node_modules/ts-jest/node_modules:/home/diogo/RocketLab/RPE/Sistema-de-Avaliacao-de-Performance/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.27.4_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_f5c72a5464527a239c0f5e9c838c2b6b/node_modules:/home/diogo/RocketLab/RPE/Sistema-de-Avaliacao-de-Performance/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
